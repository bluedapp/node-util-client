  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.4",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "chai": "^4.2.0",
    "commitizen": "^3.0.7",
    "eslint": "^5.16.0",
    "eslint-config-blued-typescript": "^1.0.1",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
